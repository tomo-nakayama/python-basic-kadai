class Human:
    # コンストラクタでnameとageを初期化
    def __init__(self, name, age):
        self.name = name
        self.age = age

    # nameとageを標準出力するメソッド
    def printinfo(self):
        print(f"名前: {self.name}, 年齢: {self.age}")

# Humanクラスのインスタンスを作成
person = Human("太郎", 25)

# printinfoメソッドを呼び出して情報を表示
person.printinfo()
